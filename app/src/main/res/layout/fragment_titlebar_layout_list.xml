<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="@color/color_white"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/wd_linear_gj"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp">

            <TextView
                style="@style/wd_text2_style"
                android:drawableLeft="@drawable/icon_my_supply"
                android:gravity="center_vertical"
                android:text="  我的供给"
                android:textColor="@color/color_balank"/>

            <TextView
                android:id="@+id/wd_text_gj"
                style="@style/wd_text2_style"
                android:layout_weight="0"
                android:drawableRight="@drawable/icon_more"
                android:text="  "/>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="@color/color_gray"></View>

        <LinearLayout
            android:id="@+id/wd_linear_qg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp">

            <TextView
                style="@style/wd_text2_style"
                android:drawableLeft="@drawable/icon_my_purchase"
                android:gravity="center_vertical"
                android:text="  我的求购"
                android:textColor="@color/color_balank"/>

            <TextView
                android:id="@+id/wd_text_qg"
                style="@style/wd_text2_style"
                android:layout_weight="0"
                android:drawableRight="@drawable/icon_more"
                android:text="  "/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="@color/color_white"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/wd_linear_jf"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/icon_my_points"
                android:gravity="center_vertical"
                android:text="  我的塑豆"
                android:textColor="@color/color_balank"
                android:textSize="@dimen/x66"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="top"
                android:text=" hot!"
                android:textColor="#f4410a"
                android:textSize="16.0sp"
                android:textStyle="italic"/>

            <TextView
                android:id="@+id/wd_text_jf"
                style="@style/wd_text2_style"
                android:layout_weight="0"
                android:drawableRight="@drawable/icon_more"
                android:text="  "/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="@color/color_white"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/wd_linear_look"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableLeft="@drawable/icon_my_points"
                android:gravity="center_vertical"
                android:text="  谁看过我"
                android:textColor="@color/color_balank"
                android:textSize="@dimen/x66"/>

            <TextView
                android:id="@+id/wd_text_look"
                style="@style/wd_text2_style"
                android:layout_weight="0"
                android:drawableRight="@drawable/icon_more"
                android:text="  "/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="@color/color_white"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/wd_linear_edu"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp">

            <TextView
                style="@style/wd_text2_style"
                android:drawableLeft="@drawable/icon_credi"
                android:gravity="center_vertical"
                android:text="  企业信用额度"
                android:textColor="@color/color_balank"/>

            <TextView
                style="@style/wd_text2_style"
                android:layout_weight="0"
                android:drawableRight="@drawable/icon_more"
                android:text="  "/>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="@color/color_gray"></View>

        <LinearLayout
            android:id="@+id/wd_linear_pz"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp">

            <TextView
                style="@style/wd_text2_style"
                android:drawableLeft="@drawable/icon_assets"
                android:gravity="center_vertical"
                android:text="  塑料配资"
                android:textColor="@color/color_balank"/>

            <TextView
                style="@style/wd_text2_style"
                android:layout_weight="0"
                android:drawableRight="@drawable/icon_more"
                android:text="  "/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="@color/color_white"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/wd_linear_set"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:drawableLeft="@drawable/icon_my_points"
                android:gravity="center_vertical"
                android:text="  设置"
                android:textColor="@color/color_balank"
                android:textSize="@dimen/x66"/>

            <TextView
                android:id="@+id/wd_text_set"
                style="@style/wd_text2_style"
                android:layout_weight="0"
                android:drawableRight="@drawable/icon_more"
                android:text="  "/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="8dp"
        android:background="@color/color_white">

        <Button
            android:id="@+id/wd_linear_tc"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@null"
            android:gravity="center"
            android:text="退出登录"
            android:textColor="@color/color_red"
            android:textSize="18.0sp"
            android:textStyle="bold"/>
    </LinearLayout>

</LinearLayout>
