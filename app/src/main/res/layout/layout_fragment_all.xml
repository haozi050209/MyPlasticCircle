<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.myplas.q.common.view.RefreshableView
        android:id="@+id/refreshableview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <com.myplas.q.common.view.MyLinearLayout
            android:id="@+id/mylinearlayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <include layout="@layout/item_listview_supdem"></include>

            <ListView
                android:id="@+id/gq_listview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:divider="@null"
                android:overScrollMode="never"
                android:scrollbars="none"></ListView>
        </com.myplas.q.common.view.MyLinearLayout>
    </com.myplas.q.common.view.RefreshableView>

    <LinearLayout
        android:id="@+id/supply_demand_prompt_linear"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:orientation="vertical"
        android:visibility="gone"></LinearLayout>
</LinearLayout>